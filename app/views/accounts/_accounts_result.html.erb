<% if accounts.present? %>
  <% accounts.each do |account| %>
    <%= link_to account_path(account), class: "no-text-decoration normal-color" do %>
      <div class="search-result-item"><%= account.name %></div>
    <% end %>
  <% end %>
<% else %>
  <div class="p-3">
    No results found.
  </div>
<% end %>
